#include <stdio.h>
#include <stdlib.h>

int main(void) {
	setbuf(stdout, NULL);

	int numero;
	int numero1;
	int numero2;

	printf ("Ingrese el primer numero");
	scanf("%d", &numero);

	printf ("Ingrese el segundo numero");
	scanf("%d", &numero1);

	printf ("Ingrese el tercer numero");
	scanf("%d", &numero2);






}
